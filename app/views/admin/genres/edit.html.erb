<h2>ジャンル編集</h2>
<div class="row">
  <%= form_with model: @genre, url: admin_genre_path(@genre), local:true do |f| %>
    <table class="table">
      <tr>
        <th>ジャンル名</th>
        <td>
          <%= f.text_field :name %>
        </td>
        <td>
          <%= f.submit "新規登録", class: "btn btn-success" %>
        </td>
      </tr>
    </table>
  <% end %>
</div>